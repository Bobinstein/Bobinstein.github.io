			counter-increment: bli-compact-counter;
			content: "" counter(bli-compact-counter, decimal-leading-zero);
			top: -30%;
			left: 30%;
			line-height: 1;
			font-size: clamp(40px, 37vw, 360px);
			font-weight: normal;
			color: #FFF;
			opacity: 0.1;
			z-index: -1;
