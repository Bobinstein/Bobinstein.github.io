	.tt-next-project.pn-image-cover-6-5 .tt-np-image::before { opacity: .65; }
	.tt-next-project.pn-image-cover-7 .tt-np-image::before { opacity: .7; }
	.tt-next-project.pn-image-cover-7-5 .tt-np-image::before { opacity: .75; }
